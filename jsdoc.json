{
  "source": {
    "include": ["./src/", "./index.js"],
    "includePattern": "\\.(js|jsx)$",
    "exclude": ["node_modules/"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "metadata": {
    "title": "js3xui Documentation"
  }
}
